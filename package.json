{
  "name": "naukhaiz-aftab-portfolio",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Modern legal portfolio for Naukhaiz Aftab - Legal Professional specializing in Constitutional Law and Policy Analysis",
  "author": "Naukhaiz Aftab",
  "license": "MIT",
  "homepage": "https://naukhaiz-aftab.github.io",
  "repository": {
    "type": "git",
